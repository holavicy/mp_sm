<!--pages/home/index.wxml-->
<view class="home">
  <view class="goods-item" wx:for="{{goodsList}}">
  <view class="image">
    <image src="{{item.imageUrl}}"></image>
  </view>
  <view class="name">
    {{item.name}}
  </view>
  <view class="time">
    {{item.time}}
  </view>
  </view>
  <tabbar selectedIndex="0"></tabbar>
</view>
